.page-header
  %h1.text-info
    #{@user.email}
    %small navigation:
%table.table
  %thead
    %tr
      %th Url
      %th Time
  %tbody
    - @events.each do |event|
      %tr
        %td= link_to event.url, event.url
        %td= local_datetime(event.created_at)
= paginate @events if @events.present?
