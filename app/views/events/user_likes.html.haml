.page-header
  %h1.text-info
    #{@user.email}
    %small likes:
- if @user.likes.present?
  %table.table
    %thead
      %tr
        %th Image
        %th Time
    %tbody
      - @user.likes.each do |like|
        %tr
          %td= link_to like.image.title, image_path(like.image)
          %td= local_datetime(like.created_at)
- else
  .panel.panel-body No INFO
